style.
  :root {
    --color-primary: #0E47A2;
  }
  html {
    padding: 0;
    margin: 0;
  }
  body {
    box-sizing: content-box;
  }
  h1 {
    color: var(--color-primary)
  }
  .text-wrapper {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
  }